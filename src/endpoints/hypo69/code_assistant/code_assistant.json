{
  "role": "doc_writer_md",
  "lang": "EN",
  "model": [ "gemini" ],
  "start_dirs": [],
  "gemini_generation_config": {
    "response_mime_type": "text/plain"
  },
  "gemini_model_name": "gemini-1.5-flash-8b",
  "openai_model_name": "gpt-4o-mini",
  "openai_assistant_id": "<OpenAI Assistant ID>",

  "exclude_dirs": [
    ".ipynb_checkpoints",
    "resources",
    "profiles",
    "sdk",
    "skd",
    "quickstart",
    "quick_start",
    "_experiments",
    "db",
    "node_modules",
    "__pycache__",
    ".git",
    ".venv",
    ".vs",
    ".vscode",
    "docs",
    "images",
    "exe",
    "pytest",
    "emil-design.com",
    "openai-assistants-quickstart"
  ],
  "exclude_files": [
    "version.py",
    "__init__.py",
    "header.py",
    "*.png",
    "*.jpg",
    "*.jpeg",
    "*.svg",
    "*.json"
  ],

  "exclude_file_patterns": [
    ".*\\(.*\\).*",
    "___{3,}.*",
    "___.*",
    ".*___.*\\..*",
    ".*___.*"
  ],
  "include_files": [
    "*.py",
    "*.js",
    "*.mjs",
    "*.htm*",
    "*.php",
    "*.txt",
    "*.md",
    "*.rst",

    "*.yaml",
    "*.yml",
    "*.ini",
    "*.cfg",
    "Makefile",
    "LICENSE*",
    "CHANGELOG*",
    "CONTRIBUTING*",
    "CODE_OF_CONDUCT*",
    "conf.py"
  ],
  "known_prefixes": [
    "```md",
    "```markdown",
    "```rst",
    "```plaintext",
    "```html",
    "```MD",
    "```MARKDOWN",
    "```RST",
    "```PALINTEXT",
    "```HTML",
    "```\n"
  ],

  "output_directory": {
    "code_checker": "<model>/consultant/<lang>/src",
    "code_explainer_md": "<model>/explainer/<lang>/md/src",
    "code_explainer_html": "<model>/explainer/<lang>/html/src",
    "pytest": "../pytest/<model>/<lang>/src",
    "doc_writer_rst": "<model>/doc/<lang>/rst/src",
    "doc_writer_md": "<model>/doc/<lang>/md/src",
    "doc_writer_html": "<model>/doc/<lang>/html/src",
    "how_to_writer": "<model>/how_to/<lang>/src"
  },
  "argparse": {
    "roles": [
      "code_checker",
      "code_explainer_md",
      "doc_writer_md",
      "pytest",
      "how_to_writer"
    ],
    "exclude-roles": [ "code_explainer_html", "doc_writer_html", "doc_writer_rst" ],
    "languages": [ "ru", "en" ],
    "model": {
      "default": [ "gemini" ],
      "choices": [ "gemini", "openai" ]
    },
    "start_dirs": {
      "default": [ "<path_to_src>" ]
    }
  }
}
