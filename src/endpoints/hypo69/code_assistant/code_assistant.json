{
  "role": "doc_writer_md",
  "lang": "EN",
  "model": [ "gemini" ],
  "start_dirs": [ "endpoints\\ai_games\\101_basic_computer_games\\ru" ],
  "gemini_generation_config": {
    "response_mime_type": "text/plain"
  },
  "gemini_model_name": "gemini-2.0-flash-exp",
  "openai_model_name": "gpt-4o-mini",
  "openai_assistant_id": "<OpenAI Assistant ID>",

  "exclude_dirs": [
    ".ipynb_checkpoints",
    "resources",
    "profiles",
    "sdk",
    "skd",
    "quickstart",
    "quick_start",
    "_experiments",
    "db",
    "node_modules",
    "__pycache__",
    ".git",
    ".venv",
    "venv",
    ".vs",
    ".vscode",
    "docs",
    "images",
    "exe",
    "pytest",
    "emil-design.com",
    "ai_games",
    "openai-assistants-quickstart",
    "arsak-zhak-programmirovanie-igr-i-golovolomok",
    "getting_started_in_classroom_computing",
    "more_basic_computer_games",
    "Understanding_Mathematics_And_Logic_Using_BASIC_Computer_Games",
    "projecteuler"

  ],
  "exclude_files": [
    "version.py",
    "header.py",
    "*.png",
    "*.jpg",
    "*.jpeg",
    "*.svg",
    "*.json",
    "*.mmd",
    "*.yaml",
    "*.yml",
    "*.ini",
    "*.cfg",
    "*.git*"
  ],

  "exclude_file_patterns": [
    ".*\\(.*\\).*",
    "___{3,}.*",
    "___.*",
    ".*___.*\\..*",
    ".*___.*"
  ],
  "include_files": [
    "*.py",
    "*.js",
    "*.mjs",
    "*.htm*",
    "*.php",
    "*.txt",
    "*.md",
    "*.rst",

    "Makefile",
    "LICENSE*",
    "CHANGELOG*",
    "CONTRIBUTING*",
    "CODE_OF_CONDUCT*",
    "conf.py"
  ],
  "remove_prefixes": [
    "```md",
    "```md\n",
    "```markdown",
    "```markdown\n",
    "```",
    "```\n"
  ],
  "known_prefixes": [
    " --------------------------- DEPRECTAED KEY --------------",
    "```md",
    "```md\n",
    "```markdown",
    "```markdown\n",
    "```",
    "```\n"
  ],
  "other_prefixes": [
    "```rst",
    "```rst\n",
    "```plaintext",
    "```html",
    "```MD",
    "```MD\n",
    "```MARKDOWN",
    "```MARKDOWN\n",
    "```RST",
    "```PALINTEXT",
    "```HTML"
  ],

  "output_directory": {
    "code_checker_md": "<model>/<lang>/consultant/src",
    "code_explainer_md": "<model>/<lang>/explainer/src",
    "code_explainer_html": "<model>/<lang>explainer/html/src",
    "doc_writer_md": "<model>/<lang>/doc/src",
    "pytest": "../pytest/<model>/src",
    "doc_writer_rst": "<model>/<lang>/doc/rst/src",
    "doc_writer_html": "<model>/<lang>/doc/html/src",
    "how_to_writer": "<model>/<lang>/how_to/src"
  },

  "roles": [
    "code_checker_md",
    "code_explainer_md",
    "doc_writer_md",

    "how_to_writer_md"
  ],
  "exclude-roles": [
    "code_explainer_html",
    "doc_writer_html",
    "doc_writer_rst",
    "pytest"
  ],
  "languages": ["ru", "en" ]


}
